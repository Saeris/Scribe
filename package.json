{
  "name": "scribe",
  "version": "0.1.1",
  "description": "A GraphQL Magic: The Gathering API Server",
  "main": "src/app.js",
  "productName": "Scribe",
  "scripts": {
    "setup": "node setup.js && npm run migrate:latest && npm run seed",
    "lint": "eslint .",
    "test": "",
    "start": "nodemon src/server.js --exec babel-node",
    "start:prod": "cross-env NODE_ENV=production nodemon src/server.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "node dist/app.js",
    "migrate:latest": "./node_modules/.bin/knex migrate:latest",
    "migrate:latest:prod": "cross-env NODE_ENV=production ./node_modules/.bin/knex migrate:latest",
    "migrate:rollback": "./node_modules/.bin/knex migrate:rollback",
    "migrate:rollback:prod": "cross-env NODE_ENV=production ./node_modules/.bin/knex migrate:rollback",
    "populate": "nodemon populate.js --exec babel-node",
    "seed": "./node_modules/.bin/knex seed:run",
    "seed:prod": "cross-env NODE_ENV=production ./node_modules/.bin/knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Saeris/Scribe.git"
  },
  "license": "MIT",
  "author": "Drake Costa <drake@saeris.io> (http://saeris.github.io/)",
  "engines": {
    "node": "6.9.1"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-import-glob": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bookshelf-modelbase": "^2.10.2",
    "cors": "^2.8.3",
    "cross-env": "^4.0.0",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "eslint": "^3.19.0",
    "glob": "^7.1.1",
    "good": "^7.1.0",
    "good-winston": "^3.1.0",
    "graphql": "^0.9.3",
    "graphql-server-hapi": "^0.7.1",
    "hapi": "^16.1.1",
    "hapi-auth-jwt2": "^7.2.3",
    "hapi-boom-decorators": "^2.2.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jsonwebtoken": "^7.1.9",
    "karma": "^1.6.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.1",
    "knex": "^0.12.6",
    "morgan": "^1.7.0",
    "mysql": "^2.12.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.1.2",
    "sqlite3": "^3.1.8",
    "test-runner-config": "^0.5.0",
    "winston": "^2.3.0",
    "winston-loggly-bulk": "^1.3.4"
  },
  "devDependencies": {
    "lodash": "^4.17.4",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "node-fetch": "^1.6.3"
  }
}
